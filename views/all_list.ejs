            <div class="table-responsive">
                <table class="table table-striped ">
                    <thead>
                        <tr>
                            <th width="60">编号</th>
                            <th width="120">姓名</th>
                            <th >内容</th>
                            <th >完成率</th>
                            <th >未完成因素</th>
                            <th >计划完成时间</th>
                            <th width="160">日期</th>
                        </tr>
                    </thead>
                    <tbody>
                    <% for(var i=0; i<result.length; i++) { %>
                        <tr>
                            <td><%=result[i].trnum %></td>
                            <td><%=result[i].name %></td>
                            <td><%=result[i].content %></td>
                            <td><%=result[i].finish_rate %></td>
                            <td><%=result[i].unfinish_reason %></td>
                            <td><%=result[i].plan_time %></td>
                            <td><%=result[i].create_time %></td>
                        </tr>
                    <% } %>
                    </tbody>
                </table>
            </div>

        </div>
